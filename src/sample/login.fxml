<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Text layoutX="263.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login">
               <font>
                  <Font size="44.0" />
               </font>
            </Text>
            <TextField fx:id="usernameField" layoutX="253.0" layoutY="164.0" promptText="Enter Username" />
            <Text layoutX="161.0" layoutY="182.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" />
            <Button fx:id="loginButton" layoutX="446.0" layoutY="164.0" mnemonicParsing="false" text="Login" onAction="#handleLoginButton"/>
      </children>
    </AnchorPane>
  </children>
</VBox>
